{
  "name": "@getflights/ember-attribute-validations",
  "version": "2.1.0",
  "description": "A package that validates models on save based on the models attributes",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "release:version": "node -p \"require('./package.json').version\"",
    "release:test": "rm -rf logs && mkdir logs && JOBS=1 CI=1 COVERAGE=true ember test ci --silent --r xunit | tee logs/xunit.xml",
    "release": "npm publish"
  },
  "repository": "https://gitlab.com/getflights/ember-attribute-validations",
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "author": "",
  "license": "MIT",
